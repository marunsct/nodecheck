{
    "_version": "1.59.0",
    "sap.app": {
        "id": "nodecheck.repositories",
        "type": "application",
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/repository/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0",
                    "localUri": "localService/metadata.xml",
                    "annotations": [
                        "annotation"
                    ]
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotations.xml",
                "settings": {
                    "localUri": "annotations/annotations.xml"
                }
            }
        },
        "crossNavigation": {
            "inbounds": {
                "repositories-display": {
                    "semanticObject": "repositories",
                    "action": "display",
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    }
                }
            }
        },
        "sourceTemplate": {
            "id": "@sap/ux-app-migrator:freestyle",
            "version": "1.19.1",
            "toolsId": "40bad6ca-89a6-4fae-9755-200fb9aa3f45"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "RepositoriesList",
                    "target": "RepositoriesList"
                },
                {
                    "pattern": "Repositories({key}):?query:",
                    "name": "RepositoriesObjectPage",
                    "target": "RepositoriesObjectPage"
                }
            ],
            "targets": {
                "RepositoriesList": {
                    "type": "Component",
                    "id": "RepositoriesList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "Repositories",
                            "variantManagement": "Page",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "ResponsiveTable",
                                        "selectionMode": "Multi",
                                        "selectAll": true
                                    },
                                    "actions": {
                                        "GetRepositories": {
                                            "press": "nodecheck.repositories.ext.controller.CustomActions.onGetRepositories",
                                            "visible": true,
                                            "enabled": true,
                                            "requiresSelection": false,
                                            "text": "{i18n>actionGetRepos}"
                                        }
                                    }
                                }
                            },
                            "navigation": {
                                "Repositories": {
                                    "detail": {
                                        "route": "RepositoriesObjectPage"
                                    }
                                }
                            },
                            "initialLoad": "Enabled"
                        }
                    }
                },
                "RepositoriesObjectPage": {
                    "type": "Component",
                    "id": "RepositoriesObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "Repositories"
                        }
                    }
                }
            },
            "config": {}
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "nodecheck.repositories.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "extends": {
            "extensions": {
                "sap.ui.controllerExtensions": {
                    "sap.fe.templates.ListReport.ListReportController": {
                        "controllerName": "nodecheck.repositories.ext.controller.ListReportExt",
                        "sap.ui.generic.app": {
                            "Repositories": {
                                "EntitySet": "Repositories",
                                "Actions": {
                                    "GetRepositories": {
                                        "id": "GetRepositories",
                                        "text": "{i18n>actionGetRepos}",
                                        "requiresSelection": false,
                                        "position": {
                                            "placement": "Before",
                                            "anchor": "RunAudit"
                                        },
                                        "press": "onGetRepositories"
                                    },
                                    "RunAudit": {
                                        "id": "RunAudit",
                                        "text": "{i18n>actionRunAudit}",
                                        "press": "onRunAudit",
                                        "requiresSelection": true,
                                        "position": {
                                            "placement": "After"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "dependencies": {
            "minUI5Version": "1.120.0",
            "libs": {
                "sap.fe.templates": {}
            }
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "nodecheck.service"
    },
    "sap.fiori": {
        "archeType": "transactional",
        "registrationIds": []
    }
}